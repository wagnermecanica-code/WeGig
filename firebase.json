{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "functions": {
    "source": "functions",
    "predeploy": ["npm --prefix \"$RESOURCE_DIR\" run lint"]
  },
  "hosting": {
    "public": "docs",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "X-Robots-Tag",
            "value": "noindex"
          }
        ]
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "to-sem-banda-83e19",
          "appId": "1:278498777601:android:3cb8437111dd6c5f9ebe00",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "to-sem-banda-83e19",
          "appId": "1:278498777601:ios:cdeec8b4da7c657a9ebe00",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "to-sem-banda-83e19",
          "configurations": {
            "android": "1:278498777601:android:3cb8437111dd6c5f9ebe00",
            "ios": "1:278498777601:ios:cdeec8b4da7c657a9ebe00",
            "web": "1:278498777601:web:8a281dd57bf8dfce9ebe00"
          }
        }
      }
    }
  }
}
